{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/Hazel/src/**",
                "${workspaceFolder}/Hazel/src/Hazel/**",
                "${workspaceFolder}/Hazel/src/Hazel/",
                "${workspaceFolder}/Hazel/vendor/spdlog/include/**",
                "${vcpkgRoot}/x64-windows/include"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "${workspaceFolder}/Hazel/src/**",
                    "${workspaceFolder}/Hazel/src/Hazel/**",
                    "${workspaceFolder}/Hazel/vendor/spdlog/include/**",
                    "${vcpkgRoot}/x64-windows/include"
                ]
            },
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "HZ_PLATFORM_WINDOWS",
                "_CRT_SECURE_NO_WARNINGS",
                "HZ_BUILD_DLL",
                "GLFW_INCLUDE_NONE",
            ],
            "windowsSdkVersion": "10.0.22000.0",
            "compilerPath": "cl.exe",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "configurationProvider": "ms-vscode.cmake-tools",
            "forcedInclude": [
                "Hazel/src/hzpch.h",
                "Hazel/src/Hazel/Core.h"
            ]
        }
    ],
    "version": 4
}